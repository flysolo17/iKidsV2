<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.danica.ikidsv2">

    <application
        android:name="com.danica.App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IKidsv2"
        tools:targetApi="31">
        <activity
            android:name=".student.UpdateProfileActivity"
            android:exported="false" />
        <activity
            android:name=".student.GameActivity"
            android:exported="false" />
        <activity
            android:name=".student.PlayActivity"
            android:exported="false" />
        <activity
            android:name=".student.LearnActivity"
            android:exported="false" />
        <activity
            android:name=".student.StudentMainActivity"
            android:exported="false" />
        <activity
            android:name=".auth.StudentSignUpActivity"
            android:exported="false" />
        <activity
            android:name=".auth.TeacherSignUpActivity"
            android:exported="false" />
        <activity
            android:name=".auth.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>